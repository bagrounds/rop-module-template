{
  "name": "serve-function-module-template",
  "version": "0.2.2",
  "main": "serve-function-module-template.js",
  "private": true,
  "scripts": {
    "start": "node bin/aFunction",
    "pretest": "npm run standard && npm run format",
    "test": "_mocha",
    "predocument": "rm -rf docs",
    "document": "jsdoc --destination docs -c .jsdoc.json -r",
    "precoverage": "rm -rf coverage",
    "coverage": "istanbul cover _mocha",
    "postcoverage": "codeclimate-test-reporter < coverage/lcov.info",
    "build": "npm run lint && npm run coverage && npm run document",
    "lint": "eslint *.js lib/**/*.js test/**/*.js",
    "standard": "standard",
    "format": "standard-format -w"
  },
  "dependencies": {
    "argparse": "^1.0.7",
    "type-check": "^0.3.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.3.1",
    "eslint": "^2.9.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.3.1",
    "eslint-plugin-standard": "^1.3.2",
    "istanbul": "^0.4.3",
    "jsdoc": "^3.4.0",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0",
    "standard": "^7.1.1",
    "standard-format": "^2.2.1"
  }
}
