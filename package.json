{
  "name": "serve-function-module-template",
  "version": "0.3.0",
  "main": "serve-function-module-template.js",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/bagrounds/serve-function-module-template.git"
  },
  "license": "MIT",
  "scripts": {
    "start": "node bin/aFunction",
    "pretest": "npm run lint",
    "test": "_mocha",
    "predocument": "npm test && rm -rf docs",
    "document": "jsdoc --destination docs -c .jsdoc.json -r",
    "precoverage": "rm -rf coverage",
    "coverage": "istanbul cover _mocha && cat ./coverage/lcov.info | coveralls",
    "postcoverage": "codeclimate-test-reporter < coverage/lcov.info",
    "build": "npm run coverage && npm run document",
    "prelint": "standard-format -w",
    "lint": "eslint --ignore-path .gitignore .",
    "gh-pages": "gh-pages-deploy"
  },
  "dependencies": {
    "type-check": "^0.3.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.3.1",
    "coveralls": "^2.11.9",
    "eslint": "^2.9.0",
    "eslint-config-standard": "^5.3.1",
    "gh-pages-deploy": "^0.4.2",
    "istanbul": "^0.4.3",
    "jsdoc": "^3.4.0",
    "mocha": "^2.4.5",
    "standard-format": "^2.2.1"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": "standard",
    "rules": {
      "valid-jsdoc": [
        2,
        {
          "requireReturn": false,
          "requireReturnType": false
        }
      ]
    }
  },
  "gh-pages-deploy": {
    "staticpath": "docs",
    "prep": [
      "document"
    ],
    "noprompt": false
  }
}
